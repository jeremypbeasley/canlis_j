// HOME

.Intro {
  position: absolute;
  left: 6vw;
  margin: 0px;
  width: calc(100% - 12vw)
  top: 5.5vw;
}
.IntroFiller {
  height: calc(100vh - 4vw);
}
.PhotosContainer {
}
.PhotoSingle {
  margin-bottom: -4px;
  position: relative;
}
.PhotoSingle img {
  max-width: 100%;
  opacity: .5;
}
.PhotoSingle .Full img {
  width: 100%;
}
  img {
  -webkit-filter: grayscale(100%);
  filter: grayscale(100%);
}
.PhotoDesc {
  position: absolute;
  top: 50%;
  transform: translateY(-50%);
  text-align: center;
  width: calc(100% - 6px);
  left: 3px;
  background: color-background;
  padding: .5rem 1rem;
}
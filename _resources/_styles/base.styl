.PageContent {
  transition: all 300ms ease-out
  opacity: 1
}
.PageContent.is-exiting {
  opacity: 0;
}

.Divider {
  width: 100%;
  height: 1px;
  background: black;
}
.LoadingShell {
  position: fixed;
  top: 0;
  width: 100%;
  height: 100vh;
  background: color-background;
  z-index: 99999;
  animation: fade_out 1000ms 2000ms ease-out forwards, des_troy 1ms 3000ms ease-out forwards;
}
.logo_building {
  transform: scale(.5);
  position: absolute;
  left: 55%;
  margin-left: -100px;
  top: 45%;
  transform: translateY(-10) scale(2);
  opacity: 0;
  animation: fade_in 800ms ease-out forwards;
  animation-delay: 500ms;
}
@media laptop {
  .logo_building {
    left: 50%;
    transform: scale(.8);
  }
}
.logo_building g {
  stroke: color-gold
}
#Stroke-1,
#Stroke-2,
#Stroke-3,
#Stroke-4,
#Stroke-5 {
  stroke-dasharray: 500;
  stroke-dashoffset: -500;
  animation: dash 1900ms cubic-bezier(0.165, 0.84, 0.44, 1) forwards;
  animation-iteration-count: 1;
}
.HomeHeadline {
  transform: translateY(10px);
  opacity: 0;
  animation: fade_in 800ms ease-out forwards, move_up 800ms ease-out forwards;
  animation-delay: 3500ms;
}

@keyframes dash {
  to {
    stroke-dashoffset: 0;
  }
}
@keyframes fade_in {
  to {
    opacity: 1;
  }
}
@keyframes fade_out {
  to {
    opacity: 0;
    transform: scale(1.05);
  }
}
@keyframes move_up {
  to {
    transform: translateY(0px);
  }
}
@keyframes des_troy {
  to {
    display: none;
    margin-left: 100000000px;
    left: 1000000px;
  }
}

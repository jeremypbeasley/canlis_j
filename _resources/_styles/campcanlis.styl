.camp-title {
  position: absolute;
  width: 90vw;
  max-width: 30em;
  z-index: 9999;
  // text-indent: -1000000px;
  // background-color: red;
  overflow: auto;
  margin-top: 120px;
  img {
    width: 100%;
  }
}

@media tablet {
  .camp-title {
    margin-top: 150px;
    width: 45vw;
    max-width: 40em;
    // height: 50vw;
    // background-color: blue;
  }
}

.camp-blurb {
  width: 100%;
  padding: 5vw 0;
  color: white;
  p {
    width: 20vw;
    text-align: center;
  }
}

.camp-location-list {
  flex-wrap: wrap;
  padding: 0 10vw;
}

@media laptop {
  .camp-location-list {
    padding: 0 15vw;
  }
}

.camp-location {
  width: 100%;
  margin: 5vw 5vw;
  text-align: center;
  &:hover {
    .camp-location-image {
      transform: rotate(5deg);
    }
  }
}

@media laptop {
  .camp-location {
    width: 20vw;
    margin: 2vw 5vw;
  }
}

.camp-location-image {
  width: 100%;
  margin-bottom: 1em;
}

.camp-location-description {
  margin-bottom: 1em;
}

.camp-location-notice {
  margin-bottom: 1em;
  text-transform: uppercase;
  font-size: .8rem;
  color: #F9A31A;
  letter-spacing: .05rem;
  /* opacity: .6; */
}

.camp-location-CTA {
  margin-top: -1vw;
  button {
    border: none;
    background-color: transparent;
    margin-top: 1vw;
    background: #F9A31A;
    padding: 14px 25px 10px 25px ;
    box-shadow: 4px 4px 0px 0px #261100;
    height: 40px;
    border-radius: 20px;
    &:hover {
      background: white;
    }
  }
}

.camp-location-CTA a {
  text-decoration: none;
}

.camp-notify {
  margin: 5vw;
  // background: blue;
  flex-flow: column;
  div {
    margin-bottom: 2vw;
  }
  p {
    color: #F9A31A
  }
}

@media tablet {
  .camp-notify {
    padding: 0 20vw;
    margin-top: 8vw;
    div {
      margin-bottom: 0;
    }
  }
}

.signup input.email {
  height: 30px;
  padding: 20px 17px;
  font-size: 14px;
  outline: none;
  // width: 80%;
  border: none;
  flex: 1;
  background: #F3E8BC;
  box-shadow: 4px 4px 0px 0px #261100;
  border-radius: 0;
}
@media (min-width: 62em) {
  .signup input.email {
    height: 50px;
    padding: 25px 17px;
    font-size: 20px;
    box-shadow: 10px 10px 0px 0px #261100;
  }
}
.signup .button {
  height: 40px;
  padding: 13px 15px;
  font-size: 15px;
  color: #261100;
  background: #F9A31A;
  outline: none;
  box-shadow: 4px 4px 0px 0px #261100;
  border-radius: 0;
}
@media (min-width: 62em) {
  .signup .button {
    height: 50px;
    padding: 16px 15px;
    font-size: 20px;
    margin-bottom: 0;
    margin-left: -2.5px;
    box-shadow: 10px 10px 0px 0px #261100;
  }
}

.signup .response a {
  color: #F9A31A;
}

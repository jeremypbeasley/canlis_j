html {
  min-height 100%
}
* {
  -webkit-tap-highlight-color rgba(0, 0, 0, 0)
  -webkit-font-smoothing antialiased
  font-smoothing antialiased
  box-sizing border-box
}
body {
  height: 100%;
  width: 100vw;
  overflow-y auto
  overflow-x hidden
  margin 0
  font-family typeface-primary
  background color-background
  color color-text
}
img {
  max-width: 100%;
  // filter: grayscale(100%);
}
.Logo {
  width: 7rem;
  height: 1.5rem;
  background-image: url("_img/canlislogo.svg");
  background-repeat: no-repeat;
  text-indent: -9999999px;
  background-size: 100%;
  display: block;
  border: none;
  position: absolute;
  top: 8vw;
  left: 6vw;
  color: color-background;
  text-align: center;
  z-index: 100;
}
@media laptop {
  .Logo {
    width: 8rem;
    height: 1.5rem;
    top: 4vw;
    left: 4vw;
    // top: 7vw;
    // left: 1.3vw;
    // transform: rotate(-90deg)
  }
}
.GlobalNav {
  position: absolute;
  top: 0vw;
  z-index: 90;
  padding-top: calc(8vw + 48px);
  padding-right: 8vw;
  right: 0vw;
  background: color-background;
  width: 100vw;
  padding-bottom: 30vh;
  opacity: 0;
  transition: opacity 200ms ease-out;
}
.GlobalNav.Open {
  opacity: 1;
}
@media laptop {
  .GlobalNav {
    background: none;
    padding-top: calc(4vw + 8px);
    padding-right: 4vw;
    display: block;
    opacity: 1;
    padding-bottom: 0;
  }
}
.GlobalNavToggle {
  text-align: right;
  cursor: pointer;
}
.GlobalNav ul {
}
.GlobalNav li {
  margin-bottom: 2rem;
}
@media laptop {
  .GlobalNav li {
    margin-bottom: 1rem;
  }
}
.GlobalNav li, .GlobalNav li a {
  border: 0px;
  text-align: right;
  transition: 200ms all ease-out;
}
.SocialNav {
  margin-top: 1rem;
}
.SocialNav li {
  float: right;
}
.SocialNav li a {
  width: 30px;
  height: 30px;
  background-size: 97%;
  background-repeat: no-repeat;
  text-indent: 99999px;
  display: block;
  margin-left: 1rem;
  transition: opacity 200ms ease-out;
}
@media laptop {
  .SocialNav li a {
    width: 22px;
    height: 22px;
    margin-left: .5rem;
  }
}
.SocialNav li a:hover {
  opacity: .5;
  border-bottom: none;
}
.SocialNav li a.Twitter {
  background-image: url("http://cdn2.dropmark.com/39456/8d64ff48f9e52a330d698b17e8ac32aea79bbbf0/icon_twitter.png")
}
.SocialNav li a.Instagram {
  background-image: url("http://cdn2.dropmark.com/39456/a0bc1cf323fb4f99e3cc26ea49d7851d819dfc0c/icon_instagram.png")
}
.NavHamburger {
  position: absolute;
  width: 15px;
  height: 14px;
  background-image: url(_img/hammyfull.svg);
  background-repeat: no-repeat;
  background-position: top center;
  background-size: 100%;
  right: 0vw;
  top: 0vw;
  margin: 8vw;
  z-index: 100;
}
.NavHamburger.Open {
  background-position: bottom center;
}
@media laptop {
  .NavHamburger {
    display: none;
  }
}
// GENERAL

.column.FirstThing {
  margin-top: 40vh;
}

// FOOTER

.GlobalFooter {
  padding-top: 6vw;
  padding-bottom: -10vw;
  // background: #F2F1EF
}
@media laptop {
  .GlobalFooter {
    margin-top: 8vw;
    padding-bottom: 5vw;
  }
}
.GlobalFooter a {
  border: none;
}
@media laptop {
  .FooterLine {
    background: color-text;
    height: 1px;
    width: 6.5vw;
    position: relative;
    margin-left: 0vw;
    top: -5vw;
  }
}
.RelaisLogo img {
  background: transparent;
  max-width: 150px;
}


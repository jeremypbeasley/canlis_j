.camp {
  background: #324401;
  min-height: 100vh;
  width: 100vw;
  overflow: auto;
  color: #F3E8BC;
  font-family: futuramaxicgbold;
  line-height: 140%;
  h1 {
    font-family: futuramaxicgbold
  }
}

.camp-eagle {
  width: 10rem;
  height: 10rem;
  background: url(/uploads/camp-eagle.gif);
  position: fixed;
  z-index: 99999;
  right: -150px;
  top: 300px;
  &.boom {
    background: url(/uploads/boom.gif);
    background-repeat: no-repeat;
    transform: scale(2);
  }
}

.camp-mountain {
  width: 100vw;
  height: 27em;
  position: relative;
  background-color: #718728;
}

@media tablet {
  .camp-mountain {
    height: 23em;
  }
}

